<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="parentVBox" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.lariscy.serverrepo.client.controller.LeftMenuController">
   <children>
      <ToggleButton fx:id="btnServerList" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#gotoServerList" styleClass="left-menu-toggle-button" textAlignment="CENTER">
         <toggleGroup>
            <ToggleGroup fx:id="leftMenuToggleGroup" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton fx:id="btnUrlList" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#gotoUrlList" styleClass="left-menu-toggle-button" textAlignment="CENTER" toggleGroup="$leftMenuToggleGroup" />
   </children>
</VBox>
